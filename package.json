{
  "name": "expense-manager",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "wrangler:dev": "vite build && wrangler dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "latest",
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@sveltejs/adapter-cloudflare": "^7.2.4",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/language-server": "latest",
    "@tailwindcss/vite": "^4.1.17",
    "@trivago/prettier-plugin-sort-imports": "latest",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "svelte": "^5.44.1",
    "svelte-check": "^4.3.4",
    "svelte-language-server": "latest",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "typescript-svelte-plugin": "latest",
    "vite": "npm:rolldown-vite@^7.2.8",
    "wrangler": "latest"
  },
  "dependencies": {
    "@auth/sveltekit": "^1.11.1",
    "@libsql/client": "^0.15.15",
    "@lucide/svelte": "latest",
    "@skeletonlabs/skeleton": "^4.5.3",
    "@skeletonlabs/skeleton-svelte": "^4.5.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "ag-charts-community": "^12.3.1",
    "ag-grid-community": "^34.3.1",
    "ag-grid-enterprise": "^34.3.1",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "zod": "^4.1.13"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}