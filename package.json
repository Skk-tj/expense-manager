{
  "name": "expense-manager",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "wrangler:dev": "vite build && wrangler dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "latest",
    "@eslint/compat": "^1.3.0",
    "@eslint/js": "^9.29.0",
    "@sveltejs/adapter-cloudflare": "^7.0.4",
    "@sveltejs/kit": "^2.21.5",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/language-server": "latest",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-svelte": "^3.9.2",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "svelte": "^5.34.1",
    "svelte-check": "^4.2.1",
    "svelte-language-server": "latest",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "typescript-svelte-plugin": "latest",
    "vite": "npm:rolldown-vite@^6.3.19",
    "wrangler": "latest"
  },
  "dependencies": {
    "@auth/sveltekit": "^1.9.2",
    "@libsql/client": "^0.15.9",
    "@lucide/svelte": "^0.515.0",
    "@skeletonlabs/skeleton": "^3.1.3",
    "@skeletonlabs/skeleton-svelte": "^1.2.3",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "ag-charts-community": "^11.3.2",
    "ag-grid-community": "^33.3.2",
    "ag-grid-enterprise": "^33.3.2",
    "drizzle-orm": "^0.44.2",
    "drizzle-zod": "^0.8.2",
    "zod": "^3.25.64"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
