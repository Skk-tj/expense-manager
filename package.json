{
  "name": "expense-manager",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:check": "drizzle-kit check",
    "wrangler:dev": "vite build && wrangler dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "latest",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.31.0",
    "@sveltejs/adapter-cloudflare": "^7.1.0",
    "@sveltejs/kit": "^2.25.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@tailwindcss/language-server": "latest",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-svelte": "^3.11.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.36.10",
    "svelte-check": "^4.3.0",
    "svelte-language-server": "latest",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "typescript-svelte-plugin": "latest",
    "vite": "npm:rolldown-vite@^7.0.9",
    "wrangler": "latest"
  },
  "dependencies": {
    "@auth/sveltekit": "^1.10.0",
    "@libsql/client": "^0.15.10",
    "@lucide/svelte": "latest",
    "@skeletonlabs/skeleton": "^3.1.7",
    "@skeletonlabs/skeleton-svelte": "^1.3.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "ag-charts-community": "^12.0.2",
    "ag-grid-community": "^34.0.2",
    "ag-grid-enterprise": "^34.0.2",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.2",
    "zod": "^4.0.5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
